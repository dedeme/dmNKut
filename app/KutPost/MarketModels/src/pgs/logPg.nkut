/// Home page.

import "libdm/log";
import "db";

/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "getLog": return "{\"Log\":" + log.readJs() + "}";
    "resetLog": {
        log.reset();
        return "{}";
      }
    default: throw "Value of rq (" + Rq.rq + ") is not valid";
  }
};
