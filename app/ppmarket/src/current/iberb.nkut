/// Current data from IBERB.

import "daily/iberb";
import "data/dailyQ";
import "data/currentQ";

/// \ -> [([<currentQ>.]|s), b]
read = async \ -> {
  QW, ok = await iberb.read();
  if (!ok) return QW;
  Qs = [];
  for (:dailyQ q = QW.Qs)
    Qs.push(currentQ.mk(q.code, q.close));

  return [Qs, true];
};
