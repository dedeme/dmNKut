/// Current 'ticks' data.

import "current/bym";
import "current/iberb";

/// \{*.} -> [([<currentQ>.]|s), b]
process = async \Rq -> {
  switch (Rq.sv) {
    "BYM": return await bym.read();
    "IBERB": return await iberb.read();
    default: throw "Server not found: " + Rq.sv;
  }
};
