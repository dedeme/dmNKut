/// File data test.

import "test";

/// \ -> ()
ibex = \ -> {
  R, ok = test.rq({
    rq: "ixs",
    sv: "IBEX"
  });
  if (!ok) {
    sys.println(R);
    return;
  }

  QsR, ok2 = js.r(R);
  if (!ok2) {
    sys.println(QsR);
    return;
  }

  Qs, Ws : QsR;
  sys.test(sys.type(Qs), "arr");
  sys.test(sys.type(Ws), "arr");
  for (w = Ws) sys.println(w);
  for (q = Qs) sys.println(q);
};

