/// 'sprs' documentation.

/// s
text = """
  <pre>
  ** Start ***

  ð = Param[0]
  ð+ = 1 + ð
  ð- = 1 - ð

  X (Maximum) = c
  N (Minimum) = cð-
  Ref = N
  T (Temporary) = -1;
  isSold = false

  ** Process ***

  !isSold
    ? c < N
      ? isSold = true
        N = c
        T = -1
        Ref = X
      : c > X
        ? X = c
          T >= 0
            ? N = T
              T = -1
            : -
        : (T < 0 & c < Xð-) | c < T
          ? T = c
          : -
        Ref = N
    : c > X
      ? isSold = false
        X = c
        T = -1
        Ref = N
      : c < N
        ? N = c
          T >= 0
            ? X = T
              T = -1
            : -
        : (T < 0 & c > Nð+) | c > T
          ? T = c
          : -
        Ref = X
  </pre>
""";
